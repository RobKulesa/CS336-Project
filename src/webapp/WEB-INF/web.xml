<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	 id="WebApp_ID" version="3.0">
  <display-name>CS336Project</display-name>
    <session-config>
        <session-timeout>10</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>JSP/LandingPage.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <display-name>LoginServlet</display-name>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.auctionsite.controllers.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <display-name>CreateUserServlet</display-name>
        <servlet-name>CreateUserServlet</servlet-name>
        <servlet-class>com.auctionsite.controllers.CreateUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateUserServlet</servlet-name>
        <url-pattern>/CreateUserServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <display-name>LogoutServlet</display-name>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.auctionsite.controllers.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <display-name>EarningsServlet</display-name>
        <servlet-name>EarningsServlet</servlet-name>
        <servlet-class>com.auctionsite.controllers.EarningsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EarningsServlet</servlet-name>
        <url-pattern>/EarningsServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GoBackServlet</servlet-name>
        <servlet-class>com.auctionsite.controllers.GoBackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GoBackServlet</servlet-name>
        <url-pattern>/GoBackServlet</url-pattern>
    </servlet-mapping>
</web-app>